filter {
	if "no_output"  not in [tags] {
		# Si on a detectÃ© une ligne rejettÃ©e par RBL on place les paramÃ¨tres suivants pour Ãªtre conforme aux lignes avec tag
		if [rbl] {
			mutate {
				add_field => {
					"as_mailaction" => "reject"
					"as_mailstatus" => "rbl"
					"as_mailtag" => "RBL"
				}
				remove_field => [rbl]
			}
		}

		mutate {
			lowercase => ["tag"]
		}

	}
}
